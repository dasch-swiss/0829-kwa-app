#
#   Attributes for the regular standoff tags
#    TODO: check if it is possible to limit the values that are allowed
#
    
:standoffTagHasRendAttribute rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf knora-base:objectCannotBeMarkedAsDeleted ;
                  rdfs:comment "describes rendition of the tag as defined in tei guidelines"@en ;
                  knora-base:subjectClassConstraint :StandoffRenderableTag ;
                  knora-base:objectDatatypeConstraint xsd:string ;
                  knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string .
                  
:standoffTagHasHandAttribute rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf knora-base:objectCannotBeMarkedAsDeleted ;
                  rdfs:comment "describes the hand responsible for the content of the element as defined in tei guidelines"@en ;
                  knora-base:subjectClassConstraint :StandoffRenderableTag ;
                  knora-base:objectDatatypeConstraint xsd:string ;
                  knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string .
                  
:standoffTagHasTypeAttribute rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf knora-base:objectCannotBeMarkedAsDeleted ;
                  rdfs:comment "describes a more detailed type of content of the element as defined in tei guidelines, eg. a lead may be a type of heading (not covered by its own tag)"@en ;
                  knora-base:subjectClassConstraint :StandoffTypedTag ;
                  knora-base:objectDatatypeConstraint xsd:string ;
                  knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string .

# an attribute to markup the part of a verse line, if the verse line is split, eg. into speech turns                  
:standoffTagHasPartAttribute rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf knora-base:objectCannotBeMarkedAsDeleted ;
                  rdfs:comment "describes the part of a verse represented by this l tag (I, M or F)"@en ;
                  knora-base:subjectClassConstraint :StandoffTeiLTag ;
                  knora-base:objectDatatypeConstraint xsd:string ;
                  knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string .
                  
# an attribute to markup the place where an addition takes place                  
:standoffTagHasNAttribute rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf knora-base:objectCannotBeMarkedAsDeleted ;
                  rdfs:comment "describes the number of a page or column"@en ;
                  knora-base:subjectClassConstraint :StandoffNumerableTag ;
                  knora-base:objectDatatypeConstraint xsd:string ;
                  knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string .
                  
# an attribute to markup the place where an addition takes place                  
:standoffTagHasEdAttribute rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf knora-base:objectCannotBeMarkedAsDeleted ;
                  rdfs:comment "describes the witness of a page or column break"@en ;
                  knora-base:subjectClassConstraint :StandoffNumerableTag ;
                  knora-base:objectDatatypeConstraint xsd:string ;
                  knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string .
                  
# an attribute to markup the place where an addition takes place                  
:standoffTagHasPlaceAttribute rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf knora-base:objectCannotBeMarkedAsDeleted ;
                  rdfs:comment "describes the part of a verse represented by this l tag (I, M or F)"@en ;
                  knora-base:subjectClassConstraint :StandoffTeiAddTag ;
                  knora-base:objectDatatypeConstraint xsd:string ;
                  knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string .
                  
#
#  The regular standoff tags
#
#  Class StandoffRenderableTag: 
#       an abstract super class for all tags with the @rend and @hand attribute (hi, p, etc.)              
:StandoffRenderableTag  rdf:type owl:Class ;
				rdfs:subClassOf knora-base:StandoffTag, [
										rdf:type owl:Restriction;
										owl:onProperty :standoffTagHasRendAttribute;
										owl:maxCardinality "1"^^xsd:nonNegativeInteger  ;
										],
										[
										rdf:type owl:Restriction;
										owl:onProperty :standoffTagHasHandAttribute;
										owl:maxCardinality "1"^^xsd:nonNegativeInteger  ;
										];
										rdfs:comment "A tag that optionally bears a rend and a hand attribute" ;
        knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string . 										
										
#
#  Class StandoffTypedTag: 
#       an abstract super class for all tags with the @type attribute (head etc)              
:StandoffTypedTag  rdf:type owl:Class ;
				rdfs:subClassOf knora-base:StandoffTag, [
										rdf:type owl:Restriction;
										owl:onProperty :standoffTagHasTypeAttribute;
										owl:maxCardinality "1"^^xsd:nonNegativeInteger  ;
										];
										rdfs:comment "A tag that optionally bears a type attribute" ;
        knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string . 										
										
#
#  Class StandoffNumerableTag: 
#       an abstract super class for all tags with the @n and @ed attributes              
:StandoffNumerableTag  rdf:type owl:Class ;
				rdfs:subClassOf knora-base:StandoffTag, [
										rdf:type owl:Restriction;
										owl:onProperty :standoffTagHasNAttribute;
										owl:maxCardinality "1"^^xsd:nonNegativeInteger  ;
										], 
										[
										rdf:type owl:Restriction;
										owl:onProperty :standoffTagHasEdAttribute;
										owl:maxCardinality "1"^^xsd:nonNegativeInteger  ;
										];
										rdfs:comment "A tag that optionally bears an n and a ed attribute" ;
        knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string . 										

#
#  The tei text tag
#
:StandoffTeiTextTag rdf:type owl:Class ;
               rdfs:subClassOf standoff:StandoffStructuralTag;			   
               rdfs:comment "marks a text, possibly within a group of texts"@en ;
     knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string .                
		
               
#
#  The tei group tag
#
:StandoffTeiGroupTag rdf:type owl:Class ;
               rdfs:subClassOf standoff:StandoffStructuralTag;			   
               rdfs:comment "marks a group of texts"@en ;
     knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string .                
               
#
#  The tei div tag
#
:StandoffTeiDivTag rdf:type owl:Class ;
               rdfs:subClassOf standoff:StandoffStructuralTag;		
               rdfs:subClassOf :StandoffTypedTag ;
               rdfs:comment "marks a division within a text"@en ;
     knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string .                
										
										
#
#
#  The tei p tag.
#
:StandoffTeiPTag rdf:type owl:Class ;
               rdfs:subClassOf standoff:StandoffParagraphTag;
			   rdfs:subClassOf :StandoffRenderableTag ;
               rdfs:comment "marks a paragraph as defined by the tei guidelines"@en ;
     knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string .                
               
#
#
#  The tei sp tag.
#
:StandoffTeiSpTag rdf:type owl:Class ;
               rdfs:subClassOf standoff:StandoffStructuralTag;
               rdfs:comment "marks a part of speech"@en ;
     knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string .                 
     
#
#
#  The tei speaker tag.
#
:StandoffTeiSpeakerTag rdf:type owl:Class ;
               rdfs:subClassOf standoff:StandoffStructuralTag;
               rdfs:subClassOf :StandoffRenderableTag ;
               rdfs:comment "marks speeker role within a part of speech"@en ;
     knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string .                

#
#
#  The tei stage tag.
#
:StandoffTeiStageTag rdf:type owl:Class ;
               rdfs:subClassOf standoff:StandoffStructuralTag;
               rdfs:subClassOf :StandoffRenderableTag ;
               rdfs:subClassOf :StandoffTypedTag ;
               rdfs:comment "marks a stage instruction in a dramatic text"@en ;
     knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string .                
               

#
#
#  The tei head tag.
#  I am not using the StandoffHxxxTags because they imply the level thingy which contradicts the tei guidelines that 
#    infer this info from the nesting of the head within divisions
#
:StandoffTeiHeadTag rdf:type owl:Class ;
               rdfs:subClassOf standoff:StandoffParagraphTag;
			   rdfs:subClassOf :StandoffRenderableTag ;
			   rdfs:subClassOf :StandoffTypedTag ;
               rdfs:comment "marks a header as defined by the tei guidelines"@en ;
               knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string .                
      
#
#
#  The tei byline tag.
#
#
:StandoffTeiBylineTag rdf:type owl:Class ;
               rdfs:subClassOf standoff:StandoffParagraphTag;
			   rdfs:subClassOf :StandoffRenderableTag ;
			   rdfs:comment "marks a byline as defined by the tei guidelines"@en ;
			   knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string . 

#
#
#  The tei pb tag.
#
#
:StandoffTeiPbTag rdf:type owl:Class ;
               rdfs:subClassOf knora-base:StandoffTag;
			   rdfs:subClassOf :StandoffNumerableTag ;
			   rdfs:comment "marks a page break in some text witness"@en ;
 			   knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string . 

#
#
#  The tei cb tag.
#
#
:StandoffTeiCbTag rdf:type owl:Class ;
               rdfs:subClassOf knora-base:StandoffTag;
               rdfs:subClassOf :StandoffNumerableTag ;
			   rdfs:comment "marks a column break in some text witness"@en ;
 			   knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string . 
			   
			   
			   
#
#
#  The tei unclear tag.
#
#
:StandoffTeiUnclearTag rdf:type owl:Class ;
               rdfs:subClassOf knora-base:StandoffTag;
			   rdfs:comment "marks a part of text as not clearly readable"@en ;
  			   knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string . 
			   
#
#  The tei lg tag. A structural tag for a strophe of verse
#
:StandoffTeiLgTag rdf:type owl:Class ;
               rdfs:subClassOf standoff:StandoffStructuralTag;
               rdfs:comment "marks a strophe of verse"@en ;
			   knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string . 
			   
#
#  The tei l tag. A structural tag for a line of verse
#
:StandoffTeiLTag rdf:type owl:Class ;
               rdfs:subClassOf standoff:StandoffStructuralTag, [rdf:type owl:Restriction;
										owl:onProperty :standoffTagHasPartAttribute;
										owl:maxCardinality "1"^^xsd:nonNegativeInteger  ;
										];
			   rdfs:subClassOf :StandoffRenderableTag ;
               rdfs:comment "marks a line of verse"@en ;
               knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string . 
               

#
#  The tei add tag. A structural tag for a part of text added to the previously existing text
#
:StandoffTeiAddTag rdf:type owl:Class ;
               rdfs:subClassOf standoff:StandoffStructuralTag, [rdf:type owl:Restriction;
										owl:onProperty :standoffTagHasPlaceAttribute;
										owl:maxCardinality "1"^^xsd:nonNegativeInteger  ;
										];
			   rdfs:subClassOf :StandoffRenderableTag ;
			   rdfs:subClassOf :StandoffTypedTag ;			   
               rdfs:comment "marks a part of text added to the previously existing text"@en ;
               knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string . 
               
#
#  The tei del tag. A structural tag for a part of text that has been marked as deleted
#
:StandoffTeiDelTag rdf:type owl:Class ;
               rdfs:subClassOf standoff:StandoffStructuralTag;
			   rdfs:subClassOf :StandoffRenderableTag ;
			   rdfs:subClassOf :StandoffTypedTag ;			   
               rdfs:comment "marks a part of a text marked for deletion"@en ;
               knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string . 
               
			   
#
#
#  The tei hi tag. A visual tag that emphasizes things, and can be rendered in various ways (thus has a @rend attribute)
#
:StandoffTeiHiTag rdf:type owl:Class ;
               rdfs:subClassOf standoff:StandoffVisualTag;
			   rdfs:subClassOf :StandoffRenderableTag ;
               rdfs:comment "marks a highlighted span of text as defined by the tei guidelines"@en ;
               knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string . 
               
               

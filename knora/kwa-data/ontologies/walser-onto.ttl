###############################################
## SURFACE
##############
## 
## A surface is a face of a textcarrier, normally a page. It is identified by  
## the bibliographic properties of the textcarrier and a page number or surface
## identifier (recto/verso, north face)
##
## Properties of SURFACE        

:surface-name rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf knora-base:hasValue, dcterms:title ;
          rdfs:label "Titel"@de ,
                     "title"@en ;
          rdfs:comment "A descriptive title of the surface" ;
          knora-base:subjectClassConstraint :Surface;
          knora-base:objectClassConstraint knora-base:TextValue ;
          salsah-gui:guiElement salsah-gui:SimpleText ;
          salsah-gui:guiAttribute "size=128" ,
                                  "maxlength=128" ;
          knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string .

:image-name  rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf  knora-base:hasValue ;
          rdfs:label "Bildname"@de ,
                     "image name."@en ;
          rdfs:comment "The name of the image of the surface" ;
          knora-base:subjectClassConstraint :Surface ;
          knora-base:objectClassConstraint knora-base:TextValue ;
          salsah-gui:guiElement salsah-gui:SimpleText ;
          salsah-gui:guiAttribute "size=10" ,
                                  "maxlength=10" ;
          knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string .    
                                                
:transcription-name  rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf  knora-base:hasValue ;
          rdfs:label "Transkriptionsdateiname"@de ,
                     "transcription file name."@en ;
          rdfs:comment "The name of the transcription file of the surface" ;
          knora-base:subjectClassConstraint :Surface ;
          knora-base:objectClassConstraint knora-base:TextValue ;
          salsah-gui:guiElement salsah-gui:SimpleText ;
          salsah-gui:guiAttribute "size=10" ,
                                  "maxlength=10" ;
          knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string .                                                 
                                                                                                                                            

:image-description  rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf  knora-base:hasValue ;
          rdfs:label "Bildinhalt"@de ,
                     "Image description."@en ;

          rdfs:comment "A descriptive string to be shown with the image, e.g. the image provenence." ;
          knora-base:subjectClassConstraint :Surface ;
          knora-base:objectClassConstraint knora-base:TextValue ;
          salsah-gui:guiElement salsah-gui:SimpleText ;
          salsah-gui:guiAttribute "size=10" ,
                                  "maxlength=10" ;
          knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string .

                                                
:image-copyright  rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf  knora-base:hasValue ;
          rdfs:label "Bildcopyright"@de ,
                     "image copyright."@en ;

          rdfs:comment "A copyright string to be shown with the image." ;
          knora-base:subjectClassConstraint :Surface ;
          knora-base:objectClassConstraint knora-base:TextValue ;
          salsah-gui:guiElement salsah-gui:SimpleText ;
          salsah-gui:guiAttribute "size=10" ,
                                  "maxlength=10" ;
          knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string .
                                                                  
:page_no  rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf  knora-base:hasValue ;
          rdfs:label "Seitenzahl"@de ,
                     "page no."@en ;
          rdfs:comment "The page number of the page (explicit or [assigned by editor])" ;
          knora-base:subjectClassConstraint :Surface ;
          knora-base:objectClassConstraint knora-base:TextValue ;
          salsah-gui:guiElement salsah-gui:SimpleText ;
          salsah-gui:guiAttribute "size=10" ,
                                  "maxlength=10" ;
          knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string .                                                

:surface-sequence-no rdf:type owl:ObjectProperty ;
		   rdfs:subPropertyOf knora-base:seqnum ;
           rdfs:label "Sequenznummer"@de ,
                      "Numéro de Séquence"@fr ,
                      "Numero di scala"@it ,
                      "Sequence number"@en ;
           rdfs:comment """Sequenznummer zur Sortierung der Seiten"""@de ,
                        """Cette \"property\" dénote la position dans un séquence."""@fr ,
                        """This property stands for the position within a set of rdered items (resoucres)"""@en ;
           knora-base:subjectClassConstraint :Surface ;
           knora-base:objectClassConstraint knora-base:IntValue ;
           salsah-gui:guiElement salsah-gui:Spinbox ;
           salsah-gui:guiAttribute "min=0" ,
                                   "max=-1" ;
           knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string . 
                                                
                                                
                                                
:part-of-textcarrier  rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf knora-base:isPartOf ;
          rdfs:label "Gehört zu Textträger"@de ,
                     "is part of textcarrier."@en ;
          rdfs:comment "The textcarrier this page belongs to" ;
          knora-base:subjectClassConstraint :Surface;
          knora-base:objectClassConstraint :Textcarrier ;
          salsah-gui:guiElement salsah-gui:Searchbox ;
          salsah-gui:guiAttribute "numprops=1" ;
          knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string .                                                
          
## Link Value Boilerplate...                                                
:part-of-textcarrierValue  rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf knora-base:isPartOfValue ; 
          rdfs:label "Gehört zu Textträger"@de ,
                     "is part of textcarrier."@en ;
          knora-base:subjectClassConstraint :Surface ;
          knora-base:objectClassConstraint knora-base:LinkValue ;
          knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string . 
          
##############
## Class SURFACE

:Surface rdf:type owl:Class ;
         #rdfs:subClassOf knora-base:StillImageRepresentation ,
         rdfs:subClassOf knora-base:Resource ,
          			[
					rdf:type owl:Restriction;
					owl:onProperty :surface-name;					
					owl:cardinality "1"^^xsd:nonNegativeInteger ;					
					salsah-gui:guiOrder "0"^^xsd:nonNegativeInteger 
          			] , 
          		[
					rdf:type owl:Restriction;
					owl:onProperty :image-name;					
					owl:cardinality "1"^^xsd:nonNegativeInteger ;					
					salsah-gui:guiOrder "1"^^xsd:nonNegativeInteger 
          			] , 	
          		[
					rdf:type owl:Restriction;
					owl:onProperty :transcription-name;					
					owl:maxCardinality "1"^^xsd:nonNegativeInteger ;					
					salsah-gui:guiOrder "1"^^xsd:nonNegativeInteger 
          			] ,
          		[
					rdf:type owl:Restriction;
					owl:onProperty :image-description;					
					owl:maxCardinality "1"^^xsd:nonNegativeInteger ;					
					salsah-gui:guiOrder "1"^^xsd:nonNegativeInteger 
          			] ,
           		[
					rdf:type owl:Restriction;
					owl:onProperty :image-copyright;					
					owl:maxCardinality "1"^^xsd:nonNegativeInteger ;					
					salsah-gui:guiOrder "1"^^xsd:nonNegativeInteger 
          			] ,
				 [
					rdf:type owl:Restriction;
					owl:onProperty :page_no;
					owl:cardinality "1"^^xsd:nonNegativeInteger  ;					
					salsah-gui:guiOrder "2"^^xsd:nonNegativeInteger] , 					
				 [
					rdf:type owl:Restriction;
					owl:onProperty :surface-sequence-no;
					owl:cardinality "1"^^xsd:nonNegativeInteger  ;					
					salsah-gui:guiOrder "3"^^xsd:nonNegativeInteger] ,	
				 [
					rdf:type owl:Restriction;
					owl:onProperty :part-of-textcarrier;
					owl:cardinality "1"^^xsd:nonNegativeInteger  ;					
					salsah-gui:guiOrder "4"^^xsd:nonNegativeInteger] ,
				 [
					rdf:type owl:Restriction;
					owl:onProperty :part-of-textcarrierValue;
					owl:cardinality "1"^^xsd:nonNegativeInteger  ;					
					salsah-gui:guiOrder "5"^^xsd:nonNegativeInteger] ;
					
         rdfs:comment
                 "A surface of a textcarrier, normally a page." ; 
         
         rdfs:label "Textträgerseite"@de ,
                     "textcarrier surface"@en ;
                     
         knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string . 
                     
##########################################################
# common properties of textual objects
##########################################################    
:hasTitle
	a owl:ObjectProperty;
	rdfs:label "has title"@en, "hat Untertitel"@de;
	rdfs:comment """Relating a resource to a title (as object) as a textual identifier of the resource."""@en;
	rdfs:subPropertyOf  knora-base:hasValue, dcterms:title; 
	knora-base:subjectClassConstraint knora-base:Resource;
	knora-base:objectClassConstraint knora-base:TextValue;
    knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string . 
    
:hasSubtitle
	a owl:ObjectProperty;
	rdfs:label "has subtitle"@en, "hat Untertitel"@de;
	rdfs:comment """Relating a resource to a subtitle (as object) as a textual identifier of the resource."""@en;
	rdfs:subPropertyOf  knora-base:hasValue, dcterms:title; 
	knora-base:subjectClassConstraint knora-base:Resource;
	knora-base:objectClassConstraint knora-base:TextValue;
    knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string .     
	
:hasName
	a owl:ObjectProperty;
	rdfs:label "has name"@en, "hat Name"@de;
	rdfs:comment """Relating a resource to a name as a textual identifier of the resource.."""@en;
	rdfs:subPropertyOf  knora-base:hasValue; 
	knora-base:subjectClassConstraint knora-base:Resource;
	knora-base:objectClassConstraint knora-base:TextValue;
    knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string .            
    
:hasShortCode a owl:ObjectProperty;
	rdfs:label "has short code"@en, "hat Sigel"@de;
	rdfs:comment """Relating a resource to a short code as a short identifier of the resource."""@en;
	rdfs:subPropertyOf knora-base:hasValue;
	knora-base:subjectClassConstraint knora-base:Resource;
	knora-base:objectClassConstraint knora-base:TextValue;
    knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string . 
	
:hasKWAVolume rdf:type owl:ObjectProperty ;
	      rdfs:subPropertyOf  knora-base:hasValue ;
          rdfs:label "KWA Band"@de ,
                     "KWA volume"@en ;
          rdfs:comment "The KWA volume this resource is relevant for" ;
          knora-base:subjectClassConstraint knora-base:Resource;
          knora-base:objectClassConstraint knora-base:TextValue ;
          salsah-gui:guiElement salsah-gui:SimpleText ;
          salsah-gui:guiAttribute "size=64" ,
                                  "maxlength=64" ;
          knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string . 
                                                
:hasKWAPageNo rdf:type owl:ObjectProperty ;
	      rdfs:subPropertyOf  knora-base:hasValue ;
          rdfs:label "KWA Seitenzahl"@de ,
                     "KWA page number"@en ;
          rdfs:comment "The KWA page number where this resource is described or named" ;
          knora-base:subjectClassConstraint knora-base:Resource;
          knora-base:objectClassConstraint knora-base:TextValue ;
          salsah-gui:guiElement salsah-gui:SimpleText ;
          salsah-gui:guiAttribute "size=64" ,
                                  "maxlength=64" ;
          knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string .	

:hasText a owl:ObjectProperty;
	rdfs:label "has text"@en, "hat Text"@de;
	rdfs:comment "A text that is found on the object described."@en,
		         "Ein Befundtext wie er auf der Resource steht oder ihr zugewiesen werden kann."@en;
	rdfs:subPropertyOf knora-base:hasValue;
	knora-base:subjectClassConstraint knora-base:Resource;
	knora-base:objectClassConstraint knora-base:TextValue;
	knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string . 
	
:hasDescription a owl:ObjectProperty;
	rdfs:label "has description"@en, "hat beschreibung"@de;
	rdfs:comment "A description of the object."@en,
		         "Eine Beschreibung des Objekts."@en;
	rdfs:subPropertyOf knora-base:hasValue, dcterms:description ; 
	knora-base:subjectClassConstraint knora-base:Resource;
	knora-base:objectClassConstraint knora-base:TextValue;
	knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string . 
	
:hasNumber
	a owl:ObjectProperty;
	rdfs:label "publication has number"@en, 
				"Publikation hat Nummer"@de;
	rdfs:comment """Relating a publication to its number."""@en;
	rdfs:subPropertyOf knora-base:hasValue;
	knora-base:subjectClassConstraint :Publication;
	knora-base:objectClassConstraint knora-base:TextValue;
	knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string . 
	
                                                
#
# Properties to create a date hint system that allows to sort textcarriers in a meaningful way
#  over all different types of textcarriers
#

# a superproperty of all dates
:hasDate  rdf:type owl:ObjectProperty ;
	      rdfs:subPropertyOf  knora-base:hasValue ;
          rdfs:label "hat Datierung"@de ,
                     "has Date"@en ;
          rdfs:comment "A date the resource can be sorted by";
          knora-base:subjectClassConstraint knora-base:Resource;
          knora-base:objectClassConstraint knora-base:DateValue ;
          knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string .

# the date property that should preferrably be used for sorting
:hasPreferredSortDate rdf:type owl:ObjectProperty ;
	      rdfs:subPropertyOf  :hasDate ;
	      rdfs:label "hat Sortierdatum"@de ,
                     "has preferred sort date"@en ;
          knora-base:subjectClassConstraint knora-base:Resource;
          knora-base:objectClassConstraint knora-base:DateValue ;
	      knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string . 

#
# A textual representation of a date as it has been found in the field
#
:hasDateBefund rdf:type owl:ObjectProperty ;
	      rdfs:subPropertyOf :hasText ;
	      rdfs:label "hat Datierungsbefund"@de ,                      
                      "has date befund"@en ;
          knora-base:subjectClassConstraint knora-base:Resource;
          knora-base:objectClassConstraint knora-base:TextValue;
          knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string . 

:isDatingHintFor rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf knora-base:hasLinkTo ;
           rdfs:label "ist Datierungshinweis zu"@de ,                      
                      "is dating hint for"@en ;
           knora-base:subjectClassConstraint :DatingHint;
           knora-base:objectClassConstraint knora-base:Resource ;                      
           knora-base:hasDefaultViewPermission knora-base:KnownUser ,
                                               knora-base:UnknownUser ,
                                               knora-base:ProjectMember ;
           knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string . 

:isDatingHintForValue rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf knora-base:hasLinkToValue ;
                rdfs:label "ist Datierungshinweis zu"@de ,                      
                     	 "is dating hint for"@en ;
                      	knora-base:subjectClassConstraint :DatingHint;
          		knora-base:objectClassConstraint knora-base:LinkValue ;
            knora-base:hasDefaultViewPermission knora-base:KnownUser  ;
           knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string . 
                                   
	      
##############################################
# Class dating hint
##############################################
:DatingHint rdf:type owl:Class ;					
            rdfs:subClassOf knora-base:Resource, 
            [
            	rdf:type owl:Restriction;
				owl:onProperty :hasDate;
				owl:cardinality "1"^^xsd:nonNegativeInteger ;
            ],
            [
            	rdf:type owl:Restriction;
				owl:onProperty :hasDateBefund;
				owl:cardinality "1"^^xsd:nonNegativeInteger ;
            ],
            [
            	rdf:type owl:Restriction;
				owl:onProperty :hasDescription;
				owl:minCardinality "0"^^xsd:nonNegativeInteger ;
            ],
            [
            	rdf:type owl:Restriction;
				owl:onProperty :isDatingHintFor;
				owl:minCardinality "1"^^xsd:nonNegativeInteger ;
			],
            [
            	rdf:type owl:Restriction;
				owl:onProperty :isDatingHintForValue;
				owl:minCardinality "1"^^xsd:nonNegativeInteger ;
			];
            
            rdfs:label "Datierungshinweis"@de ,
                     "date hint"@en ;					
					
             rdfs:comment 
                 "A dating hint is a hint that is used to date an object." ;
             knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string .     

	      
###############################################
## TEXTCARRIER
##############
## 
## The text carrier is a physical object or a convolute of physical objects
## that carries text and that is bibliographically identifiable
##
## Properties of TEXTCARRIER                                               
                                                                                   
                                                
:hasKwaId rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :hasShortCode, dcterms:identifier ;
          rdfs:label "KWA Textträger-Id"@de ,
                     "Kwa textcarrier id"@en ;
          rdfs:comment "The Id of the textcarrier in the context of the kwa. A more convenient and stable alternative to the knora-id." ;
          knora-base:subjectClassConstraint :Textcarrier;
          knora-base:objectClassConstraint knora-base:TextValue ;
          salsah-gui:guiElement salsah-gui:SimpleText ;
          salsah-gui:guiAttribute "size=128" ,
                                  "maxlength=128" ;
          knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string .                                                
                                                                  
:includeInKWAe rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf knora-base:hasValue;
          rdfs:label "in kwae sichtbar"@de ,
                     "included/visible in kwae"@en ;
          rdfs:comment "Wether the textcarrier should be visible/openable in the kwae application." ;
          knora-base:subjectClassConstraint :Textcarrier;
          knora-base:objectClassConstraint knora-base:BooleanValue ;
          knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string .  
                                                
                                                
:hasSignature a owl:ObjectProperty;
	rdfs:label "has signature"@en, "hat Signatur"@de;
	rdfs:comment """Relating a textcarrier to a library signature."""@en;
	rdfs:subPropertyOf :hasShortCode ;
	knora-base:subjectClassConstraint :Textcarrier;
	knora-base:objectClassConstraint knora-base:TextValue;
   knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string .                                             
                                                

#################
# Class TEXTCARRIER
:Textcarrier rdf:type owl:Class ;					
             rdfs:subClassOf knora-base:Resource  ,				
				 [
					rdf:type owl:Restriction;
					owl:onProperty :hasName;
					owl:cardinality "1"^^xsd:nonNegativeInteger ;
				 ],
				 [
					rdf:type owl:Restriction;
					owl:onProperty :hasDescription;
					owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
				 ],									
				 [
					rdf:type owl:Restriction ;
					owl:onProperty :hasKwaId ;
					owl:cardinality "1"^^xsd:nonNegativeInteger ; 
				 ],	
				 [
					rdf:type owl:Restriction;
					owl:onProperty :hasKWAVolume;
					owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
				 ],
				 [
					rdf:type owl:Restriction;
					owl:onProperty :hasKWAPageNo;
					owl:maxCardinality "1"^^xsd:nonNegativeInteger ;					
				 ],
				 [
					rdf:type owl:Restriction;
					owl:onProperty :includeInKWAe;
					owl:cardinality "1"^^xsd:nonNegativeInteger ;					
				 ],
				 [
					rdf:type owl:Restriction;
					owl:onProperty :isKeptAt;
					owl:minCardinality "0"^^xsd:nonNegativeInteger ;
				 ],
				 [
					rdf:type owl:Restriction;
					owl:onProperty :isKeptAtValue;
					owl:minCardinality "0"^^xsd:nonNegativeInteger ;
				 ],
				 [
					rdf:type owl:Restriction;
					owl:onProperty :hasSignature;
					owl:minCardinality "0"^^xsd:nonNegativeInteger ;
				 ],
				 [  rdf:type owl:Restriction;
					owl:onProperty :hasDate;
					owl:minCardinality "0"^^xsd:nonNegativeInteger
				 ];
					
		     rdfs:label "Textträger"@de ,
                     "textcarrier"@en ;					
					
             rdfs:comment 
                 "A textcarrier, a single object consisting of several pages, e.g. a book, an issue of a journal or a manuscript. Can contain multiple surfaces." ;
             
             knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string .                  
             

###############################################
## TEXTCONCEPT
##############
## 
## The text concept, by contrast of the textmanifest, is a concept of a text
## that can be realized in different variants on different textcarriers, e.g.
## there may be a micrographic draft, a print manuscript and a print in a journal.
## The words of the textmanifests that realize a concept may vary, but they still
## are variants of the same concept.
##
## Properties of TEXTCONCEPT

:hasKwaConceptId rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :hasShortCode, dcterms:identifier;
          rdfs:label "KWA Textkonzept-Id"@de ,
                     "Kwa textconcept id"@en ;
          rdfs:comment "An id of the textcarrier in the context of the kwa. Corresponds to the maintexts database id (if any)" ;
          knora-base:subjectClassConstraint :textconcept;
          knora-base:objectClassConstraint knora-base:TextValue ;
          salsah-gui:guiElement salsah-gui:SimpleText ;
          salsah-gui:guiAttribute "size=128" ,
                                  "maxlength=128" ;
          knora-base:hasPermissions "M knora-admin:ProjectMember|V knora-admin:KnownUser,knora-admin:UnknownUser"^^xsd:string . 

##############
## Class TEXTCONCEPT
:textconcept  rdf:type owl:Class  ;
              rdfs:subClassOf knora-base:Resource ,	 
					[
					rdf:type owl:Restriction;
					owl:onProperty :hasTitle;
					owl:cardinality "1"^^xsd:nonNegativeInteger  ;					
					salsah-gui:guiOrder "0"^^xsd:nonNegativeInteger],
					[
					rdf:type owl:Restriction;
					owl:onProperty :hasKwaConceptId;
